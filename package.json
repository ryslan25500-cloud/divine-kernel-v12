{
  "name": "divine-kernel-v12",
  "version": "12.0.0",
  "description": "Advanced bioinformatics toolkit",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "setup:db": "tsx scripts/setup-database.ts",
    "migrate": "tsx scripts/migrate-data.ts",
