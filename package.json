    "pg": "^8.11.3",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/pg": "^8.10.9",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.5",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0"
